<view class="cbs z3 b-white"  style="min-height:100vh" >


</view>
<!-- //头部 -->
<!-- //头部 -->
<view hidden="{{!pageTag}}" class="css customBar z3 buildHead" >
  <view class="rcs " style="height:{{top}}px"></view>
  <view class="rcs " style="height:32px">

      <view class="rcc h-32px ">
          <image class="h-35 w-35" mode="aspectFit" src="/images/daka.png"></image>
          <view class="fs-30 fw-700 c-word-6 f-shadow3">周边卡点</view>
      </view>

  </view>
  <view class="rcs " style="height:{{20}}px"></view>
</view>
<!-- 按钮 -->
<view hidden="{{!pageTag}}" class="ccc rightButton z3">

    
                <view class=" rcc ">
                                    <view  class=" w-90 h-90 br-90 rcc box8 b-white" bindtap="myFollow">
                                        <view class="rcc">
                                            <image class="rcc w-50 h-50 " mode="aspectFit" src="/images/yu.png"></image>
                                        </view>

                                        </view>
                </view>
                <view class="h-70"></view>
                <view class=" rcc ">
                                    <view  class=" w-90 h-90 br-90 rcc box8 b-white" bindtap="myInvite">
                                        <view class="rcc">
                                            <image class="rcc w-50 h-50 " mode="aspectFit" src="/images/care.png"></image>
                                        </view>

                                        </view>
                </view>
                <view class="h-70"></view>
                <view class=" rcc ">
                                        <view class="w-90 h-90 br-90 rcc box8 b-white" bindtap="myPk" >
                                            <view class="rcc ">
                                                <image class="rcc w-50 h-50 " mode="aspectFit" src="/images/collect.png"></image>
                                            </view>

                                        </view>
                </view>

</view>
<view hidden="{{!pageTag}}" class="ccc leftButton z3">
                <view class=" rcc ">
                                        <view  class="w-90 h-90 br-90 rcc box8 b-white" bindtap="searchLocation">
                                            <view class="rcc ">
                                                <image class="rcc w-50 h-50 " mode="aspectFit" src="/images/search.png"></image>
                                            </view>
                                        </view>
                </view>
                <view class="h-70"></view>
                <view class=" rcc ">
                                    <view  class="w-90 h-90 br-90 rcc box8 b-white" bindtap="userCenter">
                                            <image wx:if="{{user}}" class="rcc w-80 h-80 br-80 border10-2" mode="aspectFill" src="{{user.imgUrl}}"></image>
                                            <image wx:else class="rcc w-50 h-50 " mode="aspectFit" src="/images/user.png"></image>
                                    
                                    </view>
                </view>

</view>
<!-- 距离 -->
<view hidden="{{pks.length < 1}}" class="rcc z4" style="position: fixed;left:0;top:{{top}}px;width:100vw;height:32px">
    <view class="rcc pl-40 pr-40 br-20 b-publish" style="height:32px">
        <!-- <view></view> -->
        <view class="c-white fs-25 fw-700">距离{{pks[current].userLengthStr}}</view>
    </view>
</view>
<!-- 地图 -->
<view hidden="{{!pageTag}}" class="userBack2 z1" style="width: 100vw; height: 100vh;" >
    <map 
        id="myMap"
        style="width: 100vw; height: 100vh;"
        latitude="{{latitude}}"
        longitude="{{longitude}}"
        markers="{{markers}}"
        circles="{{circles}}"
        scale="{{scale}}"
        include-points="{{includePoints}}"
        show-location
    ></map>


</view>
<view class="bottomPk css z3">
     
        <view class="css z3">
            <swiper hidden="{{!pks || (pks.length < 1)}}" style="height:85vw" indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" circular="{{circular}}" vertical="{{vertical}}" current="{{current}}" bindchange="changePk" >
