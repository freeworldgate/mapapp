
<view class="css b-white" style="min-height:100vh">
    <view class="css " >
        <view class="rcs " style="height:{{top}}px"></view>
        <view class="rbc" style="height:32px">
            <view class="rcs" style="flex:1">
              <view class="rcs pl-4vw pr-4vw" >

                <view class="rcc h-32px " bindtap="back">
                <image class="rcc h-30 w-30" mode="aspectFit" src="/images/back.png"></image>
                <!-- <view class="w-20"></view> -->
                <!-- <view class="rcc fs-30 fw-700">捞人</view> -->
                </view>

              </view>
            </view>
            <view class="rcc fs-35 fw-700">我的捞人卡</view>
            <view style="flex:1"></view>
        
        </view>
        <view class="rcs " style="height:10px"></view>
    </view>

    <view wx:if="{{findUser.exist}}" class="css pl-6vw pr-6vw">
        <view class="ccc h-200">
            <!-- <view wx:if="{{findUser.statu.key===2}}" class="ccc  p-30 br-20 b-48">
                  <view class="fs-40 fw-700 c-red">{{findUser.timeExpire}}</view>
                  <view class="fs-25 fw-400 c-word-6">已打捞时间</view>
            </view> -->
            <view class="rcc">
                <view class="h-200 w-300 "  style="background-position: center;background-image: url({{createFindUserImage}});background-repeat: no-repeat;background-size: auto 130%;"></view>
                <!-- <image class="w-150 h-150" mode="aspectFit" src="{{createFindUserImage}}"></image> -->
            </view>

        </view>



        <view class="h-20"></view>
        <view class="rbc-nowrap pt-30 pb-30 b-line"> 
            <view class="rcs fs-30 fw-700">状态</view>
         
            <view class="rcc">
              <view hidden="{{findUser.statu.key!=2}}" class="rcc">
                    <view class="rcs fs-23 fw-300 c-3 pl-10 pr-10 pb-5 pt-5 b-48 br-10">已打捞{{findUser.timeExpire}}</view>
              </view>
              <view class="w-20"></view>
                
              <view wx:if="{{!findUser.statu}}" class="rus fs-30 fw-400 c-1">编辑</view>
              <view wx:else class="rus fs-30 fw-400 c-1">{{findUser.statu.name}}</view>
              <view class="rcc w-30 h-30">
                <view class="w-15 h-15 br-15 b-5"></view>
              </view>
              
              <!-- <image class="w-30 h-30" mode="aspectFit" src="/images/more.png"></image> -->
            </view>
        </view>
        <view hidden="{{!findUser.startTime||!findUser.endTime}}" class="rbc-nowrap pt-30 pb-30 b-line"> 
            <view class="rcs w-300 fs-30 fw-700">时间</view>
         
            <view class="rcc fs-30 fw-400 c-4 ">{{findUser.startTime}}~{{findUser.endTime}}</view>
        </view>
        <view class="rbc-nowrap pt-30 pb-30 b-line" bindtap="showPk" data-pk="{{findUser.pk}}"> 
            <view class="rcs w-300 fs-30 fw-700">卡点</view>
            <view class="w-50"></view>
            <view class="rcc">
              <view class="css" style="flex:1">
                <view class="rus fs-30 fw-400 c-4  line-1-start">{{findUser.pkName}}</view>
              </view>
              <image class="w-30 h-30" mode="aspectFit" src="/images/more.png"></image>
            </view>
        </view>
        <view class="css pt-30 pb-30 b-line">
          <view class="rbc-nowrap "> 
            <view class="rcs ">
              <view class="rcs fs-30 fw-700">打捞时长</view>
              <view class="w-20"></view>
              <view class="rcs fs-23 fw-300 c-3 pl-10 pr-10 pb-5 pt-5 b-48 br-10">剩余{{findUser.leftTime}}</view>
            </view>
            

            <picker class="rcc" bindchange="bindPickerChange" value="{{index}}" range="{{timeLength}}">
              <view class="rcc">
                  <view wx:if="{{findUser.findLength===0}}" class="rus fs-30 fw-300 c-4">设置</view>
                  <view wx:else class="rus fs-30 fw-300 c-4">{{findUser.findLength}}天</view>   
                  <view class="w-10"></view>
                  <image class="w-30 h-30" mode="aspectFit" src="/images/more.png"></image>
              </view>

            </picker>
          </view>
        </view>

        <view class="css pt-30 pb-30 b-line">
          <view class="rbc-nowrap "> 
            <view class="rcs w-300 fs-30 fw-700">场景描述</view>
            <view class="rcc" bindtap="editText">
                <!-- <view class="w-30"></view>
                <image class="w-30 h-30" mode="aspectFit" src="/images/reject.png"></image> -->
                <view class="rcc">
                    <view wx:if="{{findUser.text}}" class="rus fs-30 fw-300 c-4">修改</view>
                    <view wx:else class="rus fs-30 fw-300 c-4">编辑</view>   
                    <view class="w-10"></view>
                    <image class="w-30 h-30" mode="aspectFit" src="/images/more.png"></image>
                </view>
            </view>
          </view>
          <view hidden="{{!findUser.text}}" class="h-20"></view>
          <view hidden="{{!findUser.text}}" class="css b-48 p-20">
            <view class="rus  line-3-start fs-30 fw-400 c-4">{{findUser.text}}</view>
          </view>

        </view>
        <view class="rbc-nowrap pt-30 pb-30 b-line"> 
            <view class="css">
              <view class="rcs fs-30 fw-700">图片</view>
              <view class="rcs fs-23 fw-300">最多选择三张图片</view>
            </view>  
          

            <view class="rcc" bindtap="uploadImgs">
              <view class="rcc w-100 h-100 br-10 b-48">
                <image class="{{findUser.img3?'w-100 h-100 br-10':'w-100 h-100'}}" mode="aspectFill" src="{{findUser.img3?findUser.img3:emptyImage}}?x-oss-process=image/resize,w_300,m_lfit"></image>
              </view>
              
              <view class="w-10"></view>
              <view class="rcc w-100 h-100 br-10 b-48">
                <image class="{{findUser.img2?'w-100 h-100 br-10':'w-100 h-100'}}" mode="aspectFill" src="{{findUser.img2?findUser.img2:emptyImage}}?x-oss-process=image/resize,w_300,m_lfit"></image>
              </view>
              <view class="w-10"></view>
              <view class="rcc w-100 h-100 br-10 b-48">
                <image class="{{findUser.img1?'w-100 h-100 br-10':'w-100 h-100'}}" mode="aspectFill" src="{{findUser.img1?findUser.img1:emptyImage}}?x-oss-process=image/resize,w_300,m_lfit"></image>
              </view>
              
              <view class="rcc w-10"></view>
              <image class="w-30 h-30" mode="aspectFit" src="/images/more.png"></image>
            </view>
        </view>




    </view>
    <view wx:else class="cac" style="flex:1">
        
        <view class="ccc">
          <image class="w-400 h-400" mode="aspectFit" src="{{createFindUserImage}}"></image>
     
          <view class="rcc fs-60 fw-700 c-3" bindtap="create">+</view>
        </view>
        <view></view>
    </view>











</view>



<view class="ccc uploadButton">
  <!-- 没创建 -->
  <view hidden="{{!findUser.exist}}" class="css">
    <!-- <view wx:if="{{!findUser.statu}}" class="ccc">
      <view wx:if="{{findUser.findLength === 0 || findUser.text.length===0}}" class="rcc fs-30 fw-700 w-300 h-80 br-80 b-48 c-3">保存</view>
      <view wx:else class="rcc fs-30 fw-700 w-300 h-80 br-80 b-red c-white" bindtap="saveFind">保存</view>
      <view class="p-20 fs-25 fw-400 c-word-6 "></view>
    </view> -->
    <!-- <view wx:else class="rcc "> -->
      <!-- <view wx:if="{{findUser.statu.key===0}}" class="ccc">
          <view class="rcc fs-30 fw-700 w-300 h-80 br-80 {{(findUser.findLength === 0 || findUser.text.length===0)?'b-48 c-3':'b-red c-white'}} " bindtap="{{(findUser.findLength === 0 || findUser.text.length===0)?'':'startFind'}}">开捞</view>
          
          <view class="rcc pt-20">
              <view class="rcc h-70 fs-25 fw-400 c-word-6 " bindtap="clearUserFind">清除</view>
              <view class="w-50"></view>
              <view class="rcc h-70 fs-25 fw-400 c-word-6 " bindtap="saveFind">保存</view>
          </view>
      </view> -->
      <view wx:if="{{findUser.statu && findUser.statu.key===3}}" class="ccc">
          <view class="rcc fs-30 fw-700 w-300 h-80 br-80 b-red c-white" bindtap="clearUserFind">清除</view>
          <view class="rcc h-70 fs-25 fw-400 c-word-6 "></view>
      </view>
      <view wx:elif="{{findUser.statu && (findUser.statu.key===2 || findUser.statu.key===1)}}" class="ccc">
          <view class="rcc fs-30 fw-700 w-300 h-80 br-80 b-48 c-4" bindtap="giveUpUserPkFind">放弃打捞</view>
          <view class="rcc h-70 fs-25 fw-400 c-word-6 "> </view>

      </view>
      <view wx:else class="ccc">
          <view class="rcc fs-30 fw-700 w-300 h-80 br-80 {{(findUser.findLength === 0 || findUser.text.length===0)?'b-48 c-3':'b-red c-white'}} " bindtap="{{(findUser.findLength === 0 || findUser.text.length===0)?'':'startFind'}}">开捞</view>
          
          <view class="rcc pt-20">
              <view class="rcc h-70 fs-25 fw-400 c-word-6 " ></view>
              <!-- <view class="w-50"></view> -->
              <!-- <view class="rcc h-70 fs-25 fw-400 c-word-6 " bindtap="saveFind">保存</view> -->
          </view>
      </view>


    <!-- </view> -->
  </view>


  <view class="rcc h-20 fs-30 fw-300 c-3"></view>
  <view class="rcc  p-20">
    <view class="rcc fs-25 fw-300 c-3">遇见不撩</view>
    <image class="w-35 h-35 pl-10 pr-10" mode="aspectFit" src="/images/shank.png"></image>
    <view class="rcc fs-25 fw-300 c-3">卡点互捞</view>
  </view>
  


</view>



<view>

    <view class="coverAll" hidden="{{dialog.visible!=true}}"  catchtouchmove="return"></view>
    <view class="css dialogTip" hidden="{{dialog.visible!=true}}">
        <view class="css b-white br-20"   >
            <view class="cbs" style="flex:1">
                <view class="rcc">
                    <image class="w-300 h-300" mode="aspectFit" src="{{dialog.tipImg}}"></image>
                </view>
                
                <view class="css">
                    <view class="rcc fs-40 fw-700 c-tipColor pl-4vw pr-4vw line-3-center">{{dialog.title}}</view>
                    <view class="h-20"></view>
                    <view class="rcc-wrap fs-28 fw-400 c-3  pl-4vw pr-4vw  line-3-center">{{dialog.text}}</view> <view class="h-40"></view>
                </view>
                <view></view>
                <view></view>
                <view class="rcc pt-50 pb-50">
                    <view class="rcc w-300 h-80 br-80 bTipColor fs-30 fw-400 c-white"  bindtap="dialog_confirm" >知道了</view>
                </view>

            </view>


        </view>
        <!-- <view class="rcc p-100">
            <image class="w-60 h-60" mode="aspectFit" src="/images/close1.png" bindtap="operateDialog_cancel"></image>
        </view> -->
    </view>
    <view class="rcc tipDialog " hidden="{{tipDialog.visible!=true}}">
        <view class="ccc levelDialog_view">
            <view class="rcc">
                <image class="tipDialog_image" mode="aspectFit" src="/images/warn.png" wx:if="{{tipDialog.level==='warn'}}"></image>
                <image class="tipDialog_image" mode="aspectFit" src="/images/error.png" wx:elif="{{tipDialog.level==='error'}}"></image>
                <image class="tipDialog_image" mode="aspectFit" src="/images/info.png" wx:else></image>
            </view>
            <view style="height:30rpx"></view>
            <view class="rcc levelDialog_text">{{tipDialog.message}}</view>
        </view>
    </view>
    <view class="coverAll" hidden="{{operateDialog.visible!=true}}"  catchtouchmove="return"></view>
    <view class="css dialogTip" hidden="{{operateDialog.visible!=true}}">
        <view class="css b-white br-20"   >
            <!-- <view class="rcc">
                <view class="rcc absolute ">
                    <image class="w-600 h-400" mode="aspectFit" src="{{operateDialog.tipImg}}"></image>
                </view>
            </view> -->
            <view class="cbs" style="flex:1">
                <view class="rcc">
                    <image class="w-300 h-300" mode="aspectFit" src="{{operateDialog.tipImg}}"></image>
                </view>
                <view class="css">
                    <view class="rcc fs-40 fw-700 c-tipColor pl-4vw pr-4vw line-3-center">{{operateDialog.title}}</view>
                    <view class="h-20"></view>
                    <view class="rcc-wrap fs-28 fw-400 c-3  pl-4vw pr-4vw  line-3-center">{{operateDialog.text}}</view> <view class="h-40"></view>
                </view>

                <view></view>
                <view></view>

                <view class="rcc ">
                    <view class="rcc w-250 h-70 br-20 bTipColor fs-25 fw-400 c-white"  bindtap="operateDialog_confirm" >确定</view>
                </view>
                <view class="rcc p-30 fs-25 fw-400 c-3"  bindtap="operateDialog_cancel">知道了</view>

            </view>


        </view>
        <view class="rcc p-100">
            <image class="w-60 h-60" mode="aspectFit" src="/images/close1.png" bindtap="operateDialog_cancel"></image>
        </view>
    </view>
    <view class="cac pageLoadingDialog " hidden="{{pageLoadingDialog.visible!=true}}">
        <view class="ccc ">
            <view class="rcc">
                <image class="pageLoadingDialog_image" mode="aspectFit" src="/images/load.gif"></image>
            </view>
            <view class="rcc"></view>
        </view>
        <view></view>
    </view>
    <view class="cac pageLoadingDialog " hidden="{{pageLoadingDialogError.visible!=true}}">
        <view></view>
        <view class="ccc ">
            <view class="rcc">
                <image class="pageLoadingDialog_image" mode="aspectFit" src="/images/errorpage.png"></image>
            </view>
            <view class="rcc"></view>
        </view>
    </view>
    <view catchtouchmove="return" class="coverAll99" hidden="{{labelLoadingDialog.visible!=true}}"></view>
    <view catchtouchmove="return" class="rcc labelLoadingDialog " hidden="{{labelLoadingDialog.visible!=true}}">
        <view class="rcc labelLoadingDialog_view">
            <image class="labelLoadingDialog_image" mode="aspectFill" src="/images/loading.gif"></image>
            <view class="labelLoadingDialog_text">加载中</view>
        </view>
    </view>
    <view class="rcc labelLoadingDialog " hidden="{{labelLoadingDialogSuccess.visible!=true}}">
        <view class="rcc labelLoadingDialog_view">
            <view class="rcc">
                <image class="labelLoadingDialog_image" mode="aspectFit" src="/images/loadsuccess.png"></image>
            </view>
            <view class="labelLoadingDialog_text">加载成功</view>
        </view>
    </view>
    <view class="rcc labelLoadingDialog " hidden="{{labelLoadingDialogError.visible!=true}}">
        <view class="rcc labelLoadingDialog_view">
            <view class="rcc">
                <image class="labelLoadingDialog_image" mode="aspectFit" src="/images/loadfail.png"></image>
            </view>
            <view class="labelLoadingDialog_text">加载失败</view>
        </view>
    </view>


</view>











